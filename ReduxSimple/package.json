    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^4.17.15",
    "react": "16.3.2",
    "react-dom": "^16.13.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "webpack-dev-server": "^3.11.0"
